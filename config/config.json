{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "swivel-search-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/swivelSearch/SwivelSearchWebPart.js",
          "manifest": "./src/webparts/swivelSearch/SwivelSearchWebPart.manifest.json"
        }
      ]
    },
    "swivel-search-results-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/swivelSearchResults/SwivelSearchResultsWebPart.js",
          "manifest": "./src/webparts/swivelSearchResults/SwivelSearchResultsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SwivelSearchWebPartStrings": "lib/webparts/swivelSearch/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "SwivelSearchResultsWebPartStrings": "lib/webparts/swivelSearchResults/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}